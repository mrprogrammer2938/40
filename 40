import os
import time
import sys
class color:
    green = '\033[92m'
    red = '\033[91m'
    End = '\033[0m'
    org = '\033[33m'
    darkblue = '\033[34m'
    blue = '\033[96m'
    prl = '\033[95m'
    line = '\033[4m'
opt = "\n40/> "
def title():
    os.system("printf '\033]2;40\a'")
def cls():
    os.system("clear")
def menu_40():
    try:
        title()
        cls()
        banner()
        list()
    except EOFError:
        print("\nCtrl + D")
        print("\nExiting...")
        sys.exit()
def banner():
    print(color.green + """
