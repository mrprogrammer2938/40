        menu_40()
    else:
        cls()
        print(choose + color.red + " Not Found!" + color.End)
        try1()
def try1():
    try_to_menu = input("\npress Enter...")
    if try_to_menu == '':
        menu_40()
    else:
        menu_40()
if __name__ == '__main__':
    try:
        menu_40()
    except KeyboardInterrupt:
        print("\nCtrl + C")
        print("\nExiting...")
        sys.exit()